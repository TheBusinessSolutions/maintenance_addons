<?xml version="1.0"?>
<odoo>
    <data>

        <record id="hr_equipment_request_view_form_inherit_helpdesk" model="ir.ui.view" >
            <field name="name">maintenance.request.form.inherit.helpdesk_support</field>
            <field name="model">maintenance.request</field>
            <field name="inherit_id" ref="maintenance.hr_equipment_request_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='close_date']" position="after">
                    <field name="helpdesk_support_id"/>
                </xpath>
            </field>
        </record>

    </data>
 </odoo>
 
